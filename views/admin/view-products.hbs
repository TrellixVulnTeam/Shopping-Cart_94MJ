<section>
  <div class=" container mt-5">
    <div class="row">
    <a class="ml-auto btn btn-success" href="admin/add-products" role="button">add product</a>
  </div>
  <div class="mt-4">
  <table class="table">
  <thead>
    <tr>
      <th scope="col">No.</th>
      <th scope="col">Title</th>
      <th scope="col">Category</th>
      <th scope="col">price</th>
      <th scope="col">image</th>
    </tr>
  </thead>
  <tbody>
    {{#each product}}
    <tr>
      <th scope="row">1</th>
      <td>{{this.name}}</td>
      <td>{{this.category}}</td>
      <td>{{this.price}}</td>
      <td><img style="width:50px;height:50px" src="{{this.Image}}" alt="" srcset=""></td>
    </tr>
    {{/each}}
  </tbody>
</table>
</div>
</div>
</section>